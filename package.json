{
  "name": "fatum",
  "version": "1.2.0",
  "displayName": "Fatum",
  "description": "Generate random data",
  "main": "./dist/extension.js",
  "publisher": "dmytro-pustovit",
  "icon": ".docs/logo_128.jpg",
  "engines": {
    "vscode": "^1.78.0",
    "node": ">=10"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "fatum.domain",
        "category": "Fatum",
        "title": "Random: domain"
      },
      {
        "command": "fatum.email",
        "category": "Fatum",
        "title": "Random: email"
      },
      {
        "command": "fatum.firstName",
        "category": "Fatum",
        "title": "Random: firstName"
      },
      {
        "command": "fatum.fullName",
        "category": "Fatum",
        "title": "Random: fullName"
      },
      {
        "command": "fatum.int",
        "category": "Fatum",
        "title": "Random: integer"
      },
      {
        "command": "fatum.lastName",
        "category": "Fatum",
        "title": "Random: lastName"
      },
      {
        "command": "fatum.nickName",
        "category": "Fatum",
        "title": "Random: nickName"
      },
      {
        "command": "fatum.password",
        "category": "Fatum",
        "title": "Random: password"
      },
      {
        "command": "fatum.sentence",
        "category": "Fatum",
        "title": "Random: sentence"
      },
      {
        "command": "fatum.text",
        "category": "Fatum",
        "title": "Random: text"
      },
      {
        "command": "fatum.uniform",
        "category": "Fatum",
        "title": "Random: float"
      },
      {
        "command": "fatum.uuid",
        "category": "Fatum",
        "title": "Random: uuid"
      },
      {
        "command": "fatum.date",
        "category": "Fatum",
        "title": "Random: date as ISO string"
      }
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest:e2e": "npm run test-compile",
    "test:lock": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm",
    "test:lint": "eslint src tests --ext ts --no-error-on-unmatched-pattern",
    "test:security": "npm audit --production && npm run test:lock",
    "test:e2e": "node ./out/tests/runTest.js",
    "test:debt": "jscpd src --config .jscpd.json",
    "test": "npm run test:lint && npm run test:security && npm run test:e2e",
    "sort-deps": "npm remove --save example; npm remove --save-dev example",
    "semantic-release": "npx semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pustovitDmytro/fatum-vscode.git"
  },
  "author": "Dmytro Pustovit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pustovitDmytro/fatum-vscode/issues"
  },
  "homepage": "https://github.com/pustovitDmytro/fatum-vscode#readme",
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@babel/node": "^7.25.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.25.8",
    "@babel/runtime": "^7.25.7",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/lint": "^17.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/estree": "^1.0.6",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "^20.16.13",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.5.0",
    "chance": "^1.1.12",
    "conventional-changelog-eslint": "^3.0.9",
    "coveralls": "^3.1.1",
    "danger": "^11.3.1",
    "eslint": "^8.57.1",
    "eslint-config-incredible": "^2.4.4",
    "eslint-plugin-censor": "^1.5.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-no-secrets": "^0.9.1",
    "eslint-plugin-n": "^14.0.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "42.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "husky": "^8.0.3",
    "jscpd": "^3.5.10",
    "lockfile-lint": "^4.14.0",
    "mocha": "^10.7.3",
    "mocha-junit-reporter": "^2.2.1",
    "node-package-tester": "^1.3.6",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.5",
    "semantic-release-telegram": "^1.6.2",
    "semantic-release-vsce": "^5.7.4",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "uuid": "^9.0.1",
    "vsce": "^2.15.0",
    "vscode-test": "^1.6.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "cottus": "1.11.0",
    "fatum": "1.2.0",
    "myrmidon": "1.8.1",
    "winston": "3.9.0",
    "winston-vscode": "1.0.0"
  }
}
